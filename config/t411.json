{
    "name": "t411",
    "home": "http://www.t411.me/",
    "login": {
        "url": "http://www.t411.me/users/auth/",
        "headers": {
            "Referer": "http://www.t411.me/",
            "Content-Type": "application/x-www-form-urlencoded",
            "Accept": "application/json, text/javascript, */*; q=0.01",
            "X-Requested-With": "XMLHttpRequest",
            "Connection": "keep-alive"
        },
        "fieldsToFill": {
            "username": "login",
            "password": "password"
        },
        "fields": {
            "url": "/",
            "remember": "1"
        },
        "csrf": false,
        "check": "a[href='/users/logout/']"
    },
    "search": {
        "url": "http://www.t411.me/torrents/search/",
        "method": "GET",
        "headers": {
            "Referer": "http://www.t411.me/torrents/search/",
            "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,*/*;q=0.8",
            "Connection": "keep-alive"
        },
        "fieldsToFill": {
            "query": "search"
        },
        "fields": {
            "default": {
                "submit": "Recherche",
                "order": "seeders",
                "type": "desc"
            },
            "options": {
                "movie": {
                    "cat": "210",
                    "subcat": "631",
                    "term[17][]": "541"
                }
            }
        }
    },
    "parse": {
        "item": "table.results tbody tr",
        "detailsLink": "td+td a:first-child",
        "id": "td+td+td a",
        "idRegex": "id=([0-9]+)",
        "title": "td+td a:first-child",
        "size": "td+td+td+td+td+td",
        "seeders": "td+td+td+td+td+td+td+td",
        "leechers": "td+td+td+td+td+td+td+td+td"
    },
    "download": {
        "url": "http://www.t411.me/torrents/download/",
        "idField": "id"
    }
}