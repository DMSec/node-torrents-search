{
    "name": "frenchtorrentdb",
    "home": "http://www.frenchtorrentdb.com/",
    "login": {
        "url": "http://www.frenchtorrentdb.com/?section=LOGIN&ajax=1",
        "headers": {
            "Referer": "http://www.frenchtorrentdb.com/?section=LOGIN&Func=access_denied&access_needed",
            "Content-Type": "application/x-www-form-urlencoded",
            "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,*/*;q=0.8",
            "Connection": "keep-alive"
        },
        "fieldsToFill": {
            "username": "username",
            "password": "password"
        },
        "fields": {
            "url": "/",
            "remember": "1"
        },
        "csrf": false,
        "check": "a[href='/?section=LOGOUT']"
    },
    "search": {
        "url": "http://www.frenchtorrentdb.com/",
        "method": "GET",
        "headers": {
            "Referer": "http://www.frenchtorrentdb.com/?section=TORRENTS",
            "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,*/*;q=0.8",
            "Connection": "keep-alive"
        },
        "fieldsToFill": {
            "query": "name"
        },
        "fields": {
            "default": {
                "search": "Rechercher",
                "exact": "1",
                "section": "TORRENTS",
                "order_by": "seeders",
                "order": "DESC"
            },
            "options": {
                "movie": {
                    "adv_cat[m][1]": "71",
                    "group": "films",
                    "year": "",
                    "year_end": ""
                }
            }
        }
    },
    "parse": {
        "item": ".results_index .DataGrid ul",
        "detailsLink": "a.torrents_name_link",
        "title": "a.torrents_name_link",
        "size": "li.torrents_size",
        "seeders": "li.torrents_seeders",
        "leechers": "li.torrents_leechers",
        "custom": {
            "id": {
                "selector": "li.torrents_download a",
                "regex": "(&|&amp;)id=([0-9]+)"
            },
            "uid": {
                "selector": "li.torrents_download a",
                "regex": "(&|&amp;)uid=([0-9]+)"
            },
            "hash": {
                "selector": "li.torrents_download a",
                "regex": "(&|&amp;)hash=([a-z0-9]+)"
            }
        }
    },
    "download": {
        "url": "http://www.frenchtorrentdb.com/",
        "method": "GET",
        "params": {
            "static": {
                "section": "DOWNLOAD"
            },
            "custom": {
                "id": "id",
                "uid": "uid",
                "hash": "hash"
            }
        }
    }
}